{
  "name": "opennms",
  "version": "2.0.0",
  "description": "Client API for the OpenNMS network monitoring platform",
  "main": "dist/opennms.js",
  "bin": {
    "opennms": "dist/cli.node.js"
  },
  "author": "Benjamin Reed",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OpenNMS/opennms-js/issues"
  },
  "homepage": "https://github.com/OpenNMS/opennms-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenNMS/opennms-js.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-async-generator-functions": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-destructuring": "^7.6.0",
    "@babel/plugin-transform-for-of": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/ip-address": "^5.8.2",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.6.8",
    "@types/table": "^4.0.6",
    "@types/urijs": "^1.19.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-angularjs-annotate": "^0.10.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "cache-loader": "^4.1.0",
    "chai": "^4.1.2",
    "child_process": "^1.0.2",
    "eslint": "^6.1.0",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "jest-environment-node-debug": "^2.0.0",
    "parallel-webpack": "^2.3.0",
    "regenerator-runtime": "^0.13.3",
    "standard-changelog": "^2.0.12",
    "terser-webpack-plugin": "^2.1.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.5.4",
    "typedoc": "^0.15.0",
    "typedoc-plugin-external-module-name": "^2.1.0",
    "typedoc-webpack-plugin": "^1.1.4",
    "typescript": "^3.5.0",
    "urijs": "^1.19.1",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.9",
    "webpack-closure-compiler": "^2.1.6",
    "yargs": "^14.0.0",
    "yarn": "^1.17.3"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.6.0",
    "@types/btoa": "^1.2.3",
    "@types/object-hash": "^1.3.0",
    "axios": "^0.19.0",
    "btoa": "^1.2.1",
    "chalk": "^2.4.2",
    "cli-table3": "^0.5.0",
    "commander": "^3.0.0",
    "fs": "^0.0.1-security",
    "html-to-formatted-text": "^2.6.0",
    "ip-address": "^6.1.0",
    "lodash": "^4.17.15",
    "moment": "^2.19.3",
    "object-hash": "^1.3.1",
    "qs": "^6.5.0",
    "table": "^5.4.4",
    "version_compare": "^0.0.3",
    "x2js": "^3.3.1",
    "xmldom": "^0.1.27"
  },
  "resolutions": {
    "@types/marked": "^0.6.5",
    "atob": "^2.1.2",
    "babel-plugin-istanbul": "^5.2.0",
    "exec-sh": "^0.3.2",
    "chokidar": "^3.1.0",
    "deep-extend": "^0.6.0",
    "follow-redirects": "^1.9.0",
    "handlebars": "^4.2.0",
    "istanbul-lib-source-maps": "^3.0.6",
    "jest-config": "^24.9.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "lodash.merge": "^4.6.2",
    "lodash.template": "^4.5.0",
    "marked": "^0.7.0",
    "micromatch": "^3.1.10",
    "request": "^2.88.0",
    "sane": "^4.1.0",
    "sshpk": "^1.16.1",
    "terser-webpack-plugin": "^2.1.0",
    "typescript": "^3.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run lint",
      "pre-push": "commitlint --verbose --from=origin/master"
    }
  },
  "scripts": {
    "changelog": "standard-changelog -i CHANGELOG.md -s -r 1",
    "build": "webpack --define process.env.NODE_ENV=production --env production",
    "dev": "webpack --progress --colors",
    "dist": "yarn --ignore-engines lint && yarn --ignore-engines test && yarn --ignore-engines build",
    "docs": "webpack --env docs",
    "watch": "webpack --progress --colors --watch",
    "test": "jest",
    "watch-test": "jest --watch",
    "dist-test": "yarn --ignore-engines dist && yarn --ignore-engines smoke",
    "smoke": "node test/smoke.js",
    "lint": "tslint 'src/**/*.ts' && tsc --project tsconfig-check.json",
    "cli": "ts-node src/CLI.ts",
    "docker-build": "yarn --ignore-engines && yarn --ignore-engines dist-test && yarn --ignore-engines changelog && ./fix-ownership.sh",
    "docker": "docker run --rm --name opennms-js -v `pwd`:/build -w /build node:carbon yarn --ignore-engines docker-build"
  }
}
