{
  "name": "opennms",
  "version": "2.4.2-SNAPSHOT",
  "description": "Client API for the OpenNMS network monitoring platform",
  "main": "dist/opennms.js",
  "bin": {
    "opennms": "dist/cli.node.js"
  },
  "author": "Benjamin Reed",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OpenNMS/opennms-js/issues"
  },
  "homepage": "https://github.com/OpenNMS/opennms-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenNMS/opennms-js.git"
  },
  "devDependencies": {
    "@antora/cli": "^3.0.1",
    "@antora/site-generator-default": "^3.0.1",
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-async-generator-functions": "^7.16.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-transform-destructuring": "^7.16.7",
    "@babel/plugin-transform-for-of": "^7.16.7",
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@babel/plugin-transform-regenerator": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/btoa": "^1.2.3",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.startcase": "^4.4.6",
    "@types/node": "^12.20.46",
    "@types/object-hash": "^2.2.1",
    "@types/urijs": "^1.19.17",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-angularjs-annotate": "^0.10.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "cache-loader": "^4.1.0",
    "chai": "^4.3.4",
    "child_process": "^1.0.2",
    "eslint": "^8.6.0",
    "husky": "^7.0.0",
    "jest": "^27.4.7",
    "jest-environment-node-debug": "^2.0.0",
    "lodash-webpack-plugin": "^0.11.6",
    "parallel-webpack": "^2.6.0",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "standard-changelog": "^2.0.27",
    "terser-webpack-plugin": "^5.3.0",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "tslint-loader": "^3.5.4",
    "typedoc": "~0.19.0",
    "typedoc-plugin-external-module-name": "^4.0.6",
    "typescript": "~4.5.4",
    "urijs": "^1.19.1",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-closure-compiler": "^2.1.6",
    "yargs": "^17.3.1"
  },
  "jest": {
    "verbose": true,
    "detectOpenHandles": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.16.7",
    "@xmldom/xmldom": "^0.8.0",
    "axios": "^0.25.0",
    "btoa": "^1.2.1",
    "chalk": "^4.1.2",
    "commander": "^9.0.0",
    "fs": "^0.0.1-security",
    "html-to-formatted-text": "^2.6.0",
    "ip-address": "^8.1.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "object-hash": "^2.2.0",
    "qs": "^6.10.3",
    "table": "^6.8.0",
    "version_compare": "^0.0.3",
    "x2js": "^3.4.2"
  },
  "scripts": {
    "changelog": "standard-changelog -i CHANGELOG.md -s -r 1",
    "build": "webpack --mode production --env production",
    "dev": "webpack --mode=development --progress",
    "dist": "npm run lint && npm run test && npm run build && npm run docs",
    "api": "typedoc --out dist/docs/ src",
    "docs": "npm run api && antora --stacktrace generate local-site.yml",
    "watch": "webpack --progress --watch",
    "test": "jest",
    "watch-test": "jest --watch",
    "dist-test": "npm run dist",
    "smoke": "node test/smoke.js",
    "lint": "tslint 'src/**/*.ts' && tsc --project tsconfig-check.json",
    "cli": "ts-node src/CLI.ts",
    "docker-build": "npm ci && npm run dist-test && npm run changelog && ./fix-ownership.sh",
    "docker": "docker run --rm --name opennms-js -v `pwd`:/build -w /build node:carbon npm run docker-build",
    "prepare": "husky install"
  }
}
